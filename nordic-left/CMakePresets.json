{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20
    },
    "configurePresets": [
        {
            "name": "build-uhk-80-left",
            "displayName": "UHK 80 left",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "uhk-80-left",
                "BOARD_ROOT": "${sourceParentDir}/",
                "CONF_FILE": "${sourceDir}/prj.conf",
                "OVERLAY_CONFIG": "${sourceDir}/prj.conf.overlays/uhk-80-left.prj.conf"
            }
        },
        {
            "name": "build-uhk-80-right",
            "displayName": "UHK 80 right",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "uhk-80-right",
                "BOARD_ROOT": "${sourceParentDir}/",
                "CONF_FILE": "${sourceDir}/prj.conf",
                "OVERLAY_CONFIG": "${sourceDir}/prj.conf.overlays/uhk-80-right.prj.conf"
            }
        }
    ]
}
